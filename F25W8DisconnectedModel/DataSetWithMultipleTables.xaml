<Window x:Class="F25W8DisconnectedModel.DataSetWithMultipleTables"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:F25W8DisconnectedModel"
        mc:Ignorable="d"
        Title="DataSetWithMultipleTables" Height="450" Width="800">
    <Canvas>
        <Button x:Name="btnLoad" Content="Load" Canvas.Left="24" Canvas.Top="26" Height="37" Width="85" Click="btnLoad_Click"/>
        <DataGrid x:Name="grdCategories" Height="290" Width="354" d:ItemsSource="{d:SampleData ItemCount=5}" Canvas.Left="24" Canvas.Top="88" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        <DataGrid x:Name="grdProducts" Height="290" Width="354" d:ItemsSource="{d:SampleData ItemCount=5}" Canvas.Left="417" Canvas.Top="88" HorizontalAlignment="Left" VerticalAlignment="Center"/>

    </Canvas>
</Window>
